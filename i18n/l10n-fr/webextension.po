msgctxt "webext_non_https_fetch"
msgid "Something goes wrong with the webextension that attempted to use an API without https:// (%URL%)."
msgstr "Quelque chose s'est mal passé avec l'extension de navigateur qui a tenté d'utiliser une API sans https:// (%URL%)."

msgctxt "webext_save_in_arcollect"
msgid "Save in Arcollect"
msgstr "Ajouter à Arcollect"

msgctxt "webext_save_in_arcollect_failed"
msgid "Failed to save in Arcollect!"
msgstr "L'ajout dans Arcollect a échoué !"

msgctxt "webext_save_in_arcollect_retry"
msgid "Retry to save in Arcollect"
msgstr "Réésayer d'ajouter à Arcollect"

msgctxt "webext_saved_in_arcollect"
msgid "Saved"
msgstr "Enregistré"

msgctxt "webext_saving_in_arcollect"
msgid "Saving…"
msgstr "Enregistrement…"

msgctxt "webext_tweet_not_in_cache"
msgid "Tweet is not in cache, open a the tweet in a new tab or reload the page."
msgstr "Le tweet n'est pas le cache, ouvrez le tweet dans un nouvel onglet ou rechargez la page."

msgctxt "webext_twitter_account_not_in_cache"
msgid "An account is not in cache, hover over all mentions and accounts in the tweet or open them in a new tab."
msgstr "Un compte n'est pas le cache, survolez tous les comptes et mentions du tweet ou ouvrez les dans un nouvel onglet"
